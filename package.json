{
  "name": "vite_vue3_ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:prod": "vue-tsc --noEmit && vite build",
    "build:stage": "vue-tsc --noEmit && vite build --mode staging",
    "serve": "vite preview",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "element-plus": "^1.2.0-beta.5",
    "js-cookie": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.0-rc.10",
    "vue": "^3.2.16",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/js-cookie": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "babel-jest": "^27.4.2",
    "dart-sass": "^1.25.0",
    "eslint": "^8.4.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "svg-sprite-loader": "^6.0.11",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-compression": "^0.3.6",
    "vue-tsc": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  }
}
